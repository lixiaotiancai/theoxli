{
  "name": "theoxli",
  "version": "1.0.0",
  "description": "想搞点什么事情",
  "main": "index.js",
  "scripts": {
    "dev": "npx webpack serve --config ./webpack.dev.js --progress",
    "dev:server": "NODE_ENV=dev npx nest start --debug --watch",
    "build:client": "npx webpack --config ./webpack.prod.js --progress",
    "build:server": "npx nest build",
    "build:mp": "cross-env NODE_ENV=production npx webpack --config ./webpack.mp.js --progress",
    "build": "npm run build:client && npm run build:mp && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lixiaotiancai/theoxli.git"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ],
    "*.{css,scss,less,sss,json,md,html,yml,graphql,gql}": [
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "author": "李骁",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lixiaotiancai/theoxli/issues"
  },
  "homepage": "https://github.com/lixiaotiancai/theoxli#readme",
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.3.0",
    "jest": "^27.0.6",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^2.2.2",
    "mp-webpack-plugin": "^1.5.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
